                <p>
                        <textarea name="summary" rows="4" cols="50" placeholder="enter text here"></textarea>
                </p>

                <p>
                        <label for="item1">Item 1:</label>
                        <input type="checkbox" name="item1" id="item1">
                </p>

                <input type="submit" value="Submit Feedback">
        </form>
